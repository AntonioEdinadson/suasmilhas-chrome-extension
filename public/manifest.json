{
    "manifest_version": 3,
    "name": "Suasmilhas.com | Calculadora de Milhas",
    "author": "antonioedinadson",
    "description": "fepfiefefe",
    "version": "1.0",
    "action": {
        "default_popup": "./index.html"
    },
    "permissions": [
        "cookies",
        "storage",
        "tabs",
        "notifications"
    ],
    "icons": {
        "16": "./icons/icon-16.png",
        "48": "./icons/icon-48.png",
        "128": "./icons/icon-128.png"
    },
    "host_permissions": [
        "<all_urls>"
    ]
}